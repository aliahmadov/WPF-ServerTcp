<Window x:Class="WPF_ServerTcp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_ServerTcp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <StackPanel Margin="5,10,0,0" VerticalAlignment="Center" HorizontalAlignment="Left">
                <TextBlock Text="List of Users" FontSize="30" FontWeight="DemiBold">

                </TextBlock>
            </StackPanel>
            <ListBox Margin="0,10,0,0" ItemsSource="{Binding TcpClients}">
                
            </ListBox>
        </StackPanel>
    </Grid>
</Window>
